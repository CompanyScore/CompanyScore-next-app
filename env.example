# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=companyscore

# Keycloak Configuration
KEYCLOAK_URL=http://localhost:8081
KEYCLOAK_REALM=companyScore
KEYCLOAK_CLIENT_ID=companyscore-backend
KEYCLOAK_CLIENT_SECRET=your-client-secret-here
KEYCLOAK_FRONTEND_CLIENT_ID=companyscore-frontend
KEYCLOAK_FRONTEND_CLIENT_SECRET=your-frontend-client-secret-here
KEYCLOAK_ADMIN_USERNAME=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Next.js Public Keycloak Configuration
NEXT_PUBLIC_KEYCLOAK_URL=http://localhost:8081
NEXT_PUBLIC_KEYCLOAK_REALM=companyScore
NEXT_PUBLIC_KEYCLOAK_FRONTEND_CLIENT_ID=companyscore-frontend

# Backend API Configuration
NEXT_PUBLIC_BACK=http://localhost:8000

# Frontend Configuration
NEXT_PUBLIC_FRONT=http://localhost:3000

# S3 Images Configuration
NEXT_PUBLIC_S3_IMAGES=https://your-s3-bucket.s3.amazonaws.com

# Host Configuration
BACKEND_HOST=http://localhost:8000
FRONTEND_HOST=http://localhost:3000

# Development Hosts
DEV_BACKEND_HOST=http://localhost:8000
DEV_FRONTEND_HOST=http://localhost:3000

# Production Hosts
PROD_BACKEND_HOST=https://api.companyscore.net
PROD_FRONTEND_HOST=https://companyscore.net

# JWT Configuration
JWT_SECRET=your-jwt-secret-here
